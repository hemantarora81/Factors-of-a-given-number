package com.company;

public class Main {
    public static void main(String[] args) {
//Problem -- Factors of Given Number;
        int num =60;
        System.out.println("Factors of" + num + "is :");
        for (int i =1;i<=num;++i){
          if (num %i == 0){
                System.out.println(i + " ");
}
}
}
}
